{
  "manifest_version": 2,
  "name": "New Tab",
  "author": "r_d",
  "description": "Slumber transforms your new tab into a beautiful sleep time calculator.",
  "version": "1.0",
  "icons": {
	  "48": "48.png",
	  "128": "128.png"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "permissions": ["storage"],
  "chrome_url_overrides" : {
    "newtab": "index.html"
	}
}