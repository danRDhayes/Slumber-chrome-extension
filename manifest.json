{
  "manifest_version": 2,
  "name": "Sleepy",
  "description": "Sleep Time Calculator",
  "version": "1.0",
  "options_page": "options.html",
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "permissions": ["storage"],
  "chrome_url_overrides" : {
    "newtab": "index.html"
  }
}